{
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"quotes": [
			1,
			"single"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"semi": [
			2,
			"always"
		],
		"react/prop-types": 0
	},
	"env": {
		"browser": true,
		"amd": true,
		"jest": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"plugins": [
		"react"
	],
	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"parser": "babel-eslint"
}